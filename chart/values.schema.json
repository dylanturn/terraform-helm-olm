{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "required": [
      "namespaces"
    ],
    "properties": {
      "namespaces": {
        "type": "object",
        "required": [
          "management",
          "operator"
        ],
        "properties": {
          "management": {
            "type": "string",
            "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?"
          },
          "operator": {
            "type": "string",
            "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?"
          }
        }
      }
    }
  }