{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "required": [
      "name"
    ],
    "properties": {
      "lifecycle_manager_namespace": {
        "type": "string",
        "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?"
      },
      "name": {
        "type": "string",
        "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?"
      },
      "upgrade_strategy": {
        "type": "string",
        "enum": ["Default", "TechPreviewUnsafeFailForward"]
      },
      "subscriptions": {
        "type": "array"
      }
    }
  }